<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#ef6c00" />
            <gradient android:angle="-90" android:startColor="#ef6c00" android:endColor="#FFD180"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#ef6c00" />
            <solid android:color="#FFD180"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#ef6c00" />
            <gradient android:angle="-90" android:startColor="#FFD180" android:endColor="#ef6c00" />
        </shape>
    </item>
</selector>